{
  "hosting": {

    "public": "public",

    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],

    "redirects": [ {
      "source": "/",
      "destination": "https://blog.flky.dev",
      "type": 302
    }],

    "rewrites": [ {
      "source": "/**",
      "dynamicLinks": true
    }],

    "cleanUrls": true,

    "trailingSlash": false,

    "appAssociation": "AUTO"
  }
}
